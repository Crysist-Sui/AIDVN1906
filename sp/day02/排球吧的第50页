<li class=" j_thread_list clearfix" data-field='{&quot;id&quot;:5732832251,&quot;author_name&quot;:&quot;\u9752\u4e91XMA&quot;,&quot;author_nickname&quot;:null,&quot;author_portrait&quot;:&quot;tb.1.b733d605.HP72LONacZB2cywQmt2Suw&quot;,&quot;first_post_id&quot;:120120236647,&quot;reply_num&quot;:87,&quot;is_bakan&quot;:null,&quot;vid&quot;:&quot;&quot;,&quot;is_good&quot;:null,&quot;is_top&quot;:null,&quot;is_protal&quot;:null,&quot;is_membertop&quot;:null,&quot;is_multi_forum&quot;:null,&quot;frs_tpoint&quot;:null}'  data-tid='5732832251' data-thread-type="0" data-floor='27''>
            <div class="t_con cleafix">
