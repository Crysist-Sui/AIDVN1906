<li class=" j_thread_list clearfix" data-field='{&quot;id&quot;:6422949849,&quot;author_name&quot;:&quot;\u798f\u6797\u76db\u4e16&quot;,&quot;author_nickname&quot;:null,&quot;author_portrait&quot;:&quot;tb.1.7d820bf2.-kaKXac_IC1W50jD9aXM9w&quot;,&quot;first_post_id&quot;:129151117747,&quot;reply_num&quot;:0,&quot;is_bakan&quot;:null,&quot;vid&quot;:&quot;&quot;,&quot;is_good&quot;:null,&quot;is_top&quot;:null,&quot;is_protal&quot;:null,&quot;is_membertop&quot;:null,&quot;is_multi_forum&quot;:null,&quot;frs_tpoint&quot;:null}'  data-tid='6422949849' data-thread-type="0" data-floor='29''>
