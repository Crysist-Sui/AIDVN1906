<li class=" j_thread_list clearfix" data-field='{&quot;id&quot;:6454691147,&quot;author_name&quot;:&quot;\u5609\u5609\u7231\u8bf4\u8bdd&quot;,&quot;author_nickname&quot;:&quot;168168\ud83d\ude1c&quot;,&quot;author_portrait&quot;:&quot;tb.1.f35ec1f.p8UGBmipniPg0LhXADX4PQ&quot;,&quot;first_post_id&quot;:129484577855,&quot;reply_num&quot;:2,&quot;is_bakan&quot;:null,&quot;vid&quot;:&quot;&quot;,&quot;is_good&quot;:null,&quot;is_top&quot;:null,&quot;is_protal&quot;:null,&quot;is_membertop&quot;:null,&quot;is_multi_forum&quot;:null,&quot;frs_tpoint&quot;:null}'  data-tid='6454691147' data-thread-type="0" data-floor='11''>
